{
  "version": "2.1.4",
  "compilerOptions": {
    "outDir": "../../dist/gui_out",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es2016",
    "lib": [
      "dom",
      "es2015",
      "es2016"
    ],
    "jsx": "react",
    "experimentalDecorators": true,
    "baseUrl": "../../",
    "paths": {
      "gui/*": ["src/gui/*"],
      "app/*": ["src/app/*"]
    }
  },
  "include": [
    "../typings/index.d.ts",
    "./**/*.ts",
    "./**/*.tsx"
  ]
}