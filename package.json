  "description": "Bakaru, an anime player",
  "homepage": "https://github.com/bakaru/bakaru",
  "main": "app/main.js",
  "scripts": {
    "start": "electron dist/app debug",
    "watch:main": "tsc -p src/app -w",
    "watch": "webpack -w",
    "compile": "webpack --config=webpack.config.prod.js",
    "build": "node packaging/packager",
    "vendor": "node scripts/download-vendor",
    "test:app": "TS_NODE_PROJECT=src/app/tsconfig.json mocha --compilers ts:ts-node/register,tsx:ts-node/register src/app/**/*.test.ts",
    "test:gui": "TS_NODE_PROJECT=src/gui/tsconfig.json mocha --compilers ts:ts-node/register,tsx:ts-node/register src/gui/**/*.test.ts",
    "test": "npm run test:app && npm run test:gui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bakaru/bakaru.git"
  },
  "keywords": [
    "Bakaru",
    "anime",
    "player"
  ],
  "author": "Alexander Kukhta <me@thers.io>",
  "license": "CC0-1.0",
