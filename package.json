{
  "name": "Bakaru",
  "version": "0.1.0",
  "description": "Bakaru, an anime player",
  "homepage": "https://github.com/bakaru/bakaru",
  "main": "app/main.js",
  "scripts": {
    "start": "electron dist/app debug",
    "watch:main": "tsc -p src/app -w",
    "watch": "webpack -w",
    "compile": "npm run compile:pack && npm run compile:minify",
    "compile:pack": "webpack --config=webpack.config.prod.js",
    "compile:minify": "babel dist/gui/gui.js --out-file dist/gui/gui.js",
    "build": "node packaging/packager",
    "vendor": "node scripts/download-vendor",
    "test:app": "TS_NODE_PROJECT=src/app/tsconfig.json mocha --compilers ts:ts-node/register src/app/**/*.test.ts",
    "test-win:app": "mocha --compilers ts:ts-node/register -p src/app/tsconfig.json src/app/**/*.test.ts",
    "test:gui": "mocha src/gui/**/*.test.js",
    "test": "npm run test:app && npm run test:gui",
    "electron-rebuild": "electron-rebuild -w uws"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bakaru/bakaru.git"
  },
  "keywords": [
    "Bakaru",
    "anime",
    "player"
  ],
  "author": "Alexander Kukhta <me@thers.io>",
  "license": "CC0-4.0",
  "devDependencies": {
    "@types/bluebird": "^3.0.35",
    "@types/chai": "^3.4.34",
    "@types/debug": "0.0.29",
    "@types/electron": "^1.4.27",
    "@types/express": "^4.0.34",
    "@types/mkdirp": "^0.3.29",
    "@types/mocha": "^2.2.34",
    "@types/mock-fs": "^3.6.30",
    "@types/node": "^6.0.48",
    "@types/rimraf": "^0.0.28",
    "@types/uws": "^0.13.0",
    "@types/ws": "0.0.40",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-inferno": "^3.2.0",
    "babel-plugin-remove-comments": "^2.0.0",
    "babel-preset-babili": "0.0.9",
    "babel-preset-electron": "^0.37.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "chai-shallow-deep-equal": "^1.4.4",
    "chalk": "^1.1.3",
    "clean-webpack-plugin": "^0.1.14",
    "css-loader": "^0.26.1",
    "decompress": "^4.0.0",
    "electron": "1.4.7",
    "electron-mocha": "^3.3.0",
    "electron-packager": "^8.3.0",
    "electron-rebuild": "^1.5.7",
    "elegant-spinner": "^1.0.1",
    "file-loader": "^0.9.0",
    "got": "6.2.0",
    "html-webpack-plugin": "^2.24.1",
    "jest": "^17.0.3",
    "json-loader": "^0.5.4",
    "log-update": "^1.0.2",
    "mocha": "^3.1.2",
    "mock-fs": "^3.12.1",
    "node-cp": "^0.1.1",
    "node-sass": "^4.5.2",
    "progress": "1.1.8",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.1",
    "ts-node": "^3.0.2",
    "typescript": "^2.1.4",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "arson": "0.2.3",
    "bluebird": "^3.4.6",
    "classnames": "2.2.5",
    "color": "0.11.1",
    "debug": "^2.3.3",
    "double-ended-queue": "2.1.0-0",
    "express": "^4.14.0",
    "fast-levenshtein": "1.1.0",
    "fastpriorityqueue": "^0.1.0",
    "fs-extra": "^1.0.0",
    "inferno": "^1.3.1",
    "inferno-compat": "^3.1.1",
    "inferno-component": "^3.1.0",
    "inferno-create-element": "^3.1.0",
    "inferno-redux": "^1.3.1",
    "javascript-natural-sort": "0.7.1",
    "js-sha256": "0.3.0",
    "mkdirp": "^0.5.1",
    "mousetrap": "1.5.3",
    "mpv.js": "^0.2.0",
    "react-svg-morph": "^0.1.10",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "uws": "^0.14.5",
    "ws": "^2.3.1"
  }
}
